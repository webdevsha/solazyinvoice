modules = ["python-3.11"]
[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Lazy Invoice"

[[workflows.workflow]]
name = "Lazy Invoice"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd solazyinvoice && python3 -m http.server 5000 --bind 0.0.0.0"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
